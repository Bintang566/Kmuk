    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>XOR_crypter</AssemblyName>
    <ApplicationIcon>906308.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="906308.ico" />
